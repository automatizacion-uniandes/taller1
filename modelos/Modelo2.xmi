    <preguntas xsi:type="Encuentas:Unica" enunciado="¿usted termino todos sus estudios?">
      <opciones valor="si "/>
      <opciones valor="no"/>
    </preguntas>
    <preguntas xsi:type="Encuentas:Abierta" enunciado="¿Cual es el nombre de su universidad?"/>
