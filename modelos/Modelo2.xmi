      <opciones valor="A termino fijo"/>
      <opciones valor="Prestacion de servicios"/>
      <opciones valor="A termino indefinido"/>
    </preguntas>
    <preguntas xsi:type="Encuentas:Unica" enunciado="¿Cotiza salud y pension?">
      <opciones valor="si"/>
      <opciones valor="no"/>
    </preguntas>
